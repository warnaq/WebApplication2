@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <link rel="stylesheet" href="//stackpath.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
    <script src="//code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="register.js" type="text/javascript"></script>
    <link rel="stylesheet" href="site.css">
    <title>Registrer deg</title>

</head>

<body>
    <header id="header">
            <div class="logo">
            <img src=https://cdn.dribbble.com/users/832300/screenshots/12679063/media/337d6f161330e10ad22f2e2e072084c5.jpg height="100px" width="100px" />
        
        <nav>
                <ul class="list"> 
                <li><a href="index.cshtml">Om oss</a></li>
                <li><a href="page2.cshtml">Oversikt over alle observasjoner</a></li>
            </ul>
        </nav>    
        </div>
    </header>

    <h1>UFO Observasjon</h1>
<br>
    <p>
     <h2> Har du observert en UFO? Register dette i norges største observasjons database. </h2>
    </p>

        <div class="container">
        <form class="form">
            <div class="form-group">
                <label>Fornavn</label>
                <input type="text" id="Fornavn" />
            </div>
            <div class="form-group">
                <label>Etternavn</label>
                <input type="text" id="Etternavn" />
            </div>
            <div class="form-group">
                <label>Email</label>
                <input type="text" id="Email" />
            </div>
            <div class="form-group">
                <input type="button" id="reg" value="Registrer" onClick="lagrePerson()" class="btn btn-primary" />
            </div>
            <div id="feil" style="color: red"></div>
        </form>
    </div>
</body>
</html>
